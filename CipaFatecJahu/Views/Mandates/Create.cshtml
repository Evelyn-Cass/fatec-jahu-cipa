@model CipaFatecJahu.Models.Mandate
@{
    ViewData["Title"] = "Create";
}
@section Styles {
    <link rel="stylesheet" href="~/css/Shared/form.css" />
    <link rel="stylesheet" href="~/css/Mandates/create.css" />
}
<section class="mainbody">
    <div class="form-geral">
        <form class="form-base" method="post" asp-action="CreateAta">
            <div class="form-header">
                <h1>Nova Gestão</h1>
            </div>
            <div class="form-body mandate">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="StartYear" class="control-label"></label>
                    <input asp-for="StartYear" class="form-control" type="month" />
                    <span asp-validation-for="StartYear" class="control-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="TerminationYear" class="control-label"></label>
                    <input asp-for="TerminationYear" class="form-control" type="month"/>
                    <span asp-validation-for="TerminationYear" class="control-danger"></span>
                </div>
                <div class="btn-container">
                    <input type="submit" class="button-green button-create">
                </div>
            </div>
        </form>
    </div>

</section>
@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
